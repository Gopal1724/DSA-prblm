step1:check if all numbers in array are greater than target if yes return n 
step 2:if nums are less than target return index where it bcan be fitted 

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int n= nums.size();
        for(int i=0;i<n;i++){
            if( nums[i]>=target)// Return index where target can be inserted
           return i;
        
    }
    return n;// If target is greater than all elements, return n
    }
};
