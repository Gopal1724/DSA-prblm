void push(stack<int>& s, int a){
	s.push(a);
}

bool isFull(stack<int>& s,int n){
	// Your code goes here
	if(s.size()==n)
	return true;
	else
	return false;
}

bool isEmpty(stack<int>& s){
	// Your code goes here
 if(s.empty())
 	return  true;
 	else
 	return false;
}

int pop(stack<int>& s){
	// Your code goes here
	s.pop();
}

int getMin(stack<int>& s){
	// Your code goes here
	int mini=s.top();
	while(!s.empty()){
	    mini=min(s.top(),mini);
	    s.pop();
	}
	return mini;
	
	
	
	
	
	
}
